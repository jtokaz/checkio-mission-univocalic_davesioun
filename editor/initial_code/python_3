def davesioun(x):
    return x/10

# this assertion should be stripped after self-testing.
if __name__ == '__main__':
    assert davasaan(0) == 0, "First"
    assert davasaan(7) == 0.7, "Second"
    assert davasaan(81) == 8.1, "Third"
